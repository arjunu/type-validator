{
  "name": "typoscope",
  "version": "1.0.1",
  "description": "A runtime type schema validator for compound Javascript objects",
  "main": "lib/index.js",
  "scripts": {
    "build": "babel src --out-dir lib",
    "test": "NODE_ENV=test mocha --compilers js:babel-core/register ./tests/*.tests.js",
    "test:watch": "NODE_ENV=test mocha --compilers js:babel-core/register --recursive --watch ./tests/*.tests.js",
    "test:cov": "babel-node ./node_modules/.bin/isparta cover ./node_modules/.bin/_mocha -- --recursive ./tests/*.tests.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arjunu/type-validator.git"
  },
  "author": "Arjun U.",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/arjunu/typoscope/issues"
  },
  "homepage": "https://github.com/arjunu/typoscope#readme",
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-core": "^6.5.2",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "isparta": "^4.0.0",
    "mocha": "^2.4.5",
    "mocha-sinon": "^1.1.5",
    "sinon": "^1.17.3",
    "wallaby-webpack": "0.0.13",
    "webpack": "^1.12.13"
  },
  "dependencies": {}
}
